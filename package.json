{
  "name": "ftjs",
  "version": "0.5.6",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=9.12.0"
  },
  "scripts": {
    "build": "turbo run build",
    "docs:dev": "pnpm --filter @ftjs/doc docs:dev",
    "docs:build": "pnpm --filter @ftjs/doc docs:build",
    "semantic-release": "semantic-release",
    "semantic-release:all": "pnpm -r --workspace-concurrency=1 exec semantic-release",
    "release:tag": "git tag v$npm_config_version && git push origin v$npm_config_version",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "pnpm --filter @ftjs/core test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "bumpp": "^10.0.3",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "catalog:",
    "semantic-release": "^24.2.5",
    "semantic-release-monorepo": "^8.0.2",
    "tsx": "^4.19.3",
    "turbo": "^2.4.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,css,less,scss,json,md}": [
      "prettier --write"
    ]
  }
}
