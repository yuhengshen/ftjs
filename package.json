{
  "name": "tf",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=9.12.0"
  },
  "scripts": {
    "play": "pnpm --filter tf-playground dev",
    "build-core": "turbo run build --filter tf-core",
    "build-antd": "turbo run build --filter tf-antd",
    "release-core": "pnpm --filter tf-core release",
    "release-antd": "pnpm --filter tf-antd release",
    "release-ele": "pnpm --filter tf-element release",
    "pub-core": "pnpm --filter tf-core pub",
    "pub-antd": "pnpm --filter tf-antd pub",
    "pub-ele": "pnpm --filter tf-element pub",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "bumpp": "^10.0.3",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "catalog:",
    "turbo": "^2.4.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,css,less,scss,json,md}": [
      "prettier --write"
    ]
  }
}
